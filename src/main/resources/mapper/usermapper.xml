<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="hiccup.hiccupstore.user.dao.UserMapper">
    <insert id="save" parameterType="hiccup.hiccupstore.user.dto.UserDto">
        insert into user (userName ,nickName ,address ,email ,birth ,phone ,password)
        values(#{userName} ,#{nickName} ,#{address} ,#{email} ,#{birth} ,#{phone} ,#{password})
    </insert>
</mapper>